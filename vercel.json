{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "/(.*).(js|css|png|jpg)",
      "headers": [
        { "key": "Vercel-CDN-Cache-Control", "value": "max-age=86400" }
      ]
    }
  ]
}
